{
    "name": "sysatom/devhub",
    "description": "devhub.io",
    "keywords": [
        "devhub"
    ],
    "license": "Private",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.4.*",
        "laravel/scout": "^1.0",
        "laravel/socialite": "^2.0",
        "laracasts/flash": "~2.0",
        "roumen/sitemap": "2.6.*",
        "artesaos/seotools": "0.9.*",
        "backup-manager/laravel": "^1.1",
        "prettus/l5-repository": "^2.6",
        "arcanedev/log-viewer": "~3.0",
        "spatie/laravel-analytics": "^2.1",
        "erusev/parsedown": "^1.6",
        "knplabs/github-api": "^1.7",
        "embed/embed": "^2.7",
        "guzzlehttp/guzzle": "^6.2",
        "league/fractal": "^0.13.0",
        "mcamara/laravel-localization": "^1.1",
        "rollbar/rollbar": "~0.18.0",
        "league/flysystem-dropbox": "^1.0",
        "pragmarx/google2fa": "^0.7.1",
        "zizaco/entrust": "^1.7",
        "predis/predis": "^1.1",
        "intervention/image": "^2.3",
        "venturecraft/revisionable": "^1.28",
        "alt-three/badger": "^3.1",
        "roumen/feed": "^2.10",
        "mailgun/mailgun-php": "^2.1",
        "mews/captcha": "^2.1",
        "laravel-notification-channels/pushover": "^0.0.2",
        "hieu-le/active": "^3.3",
        "spatie/laravel-url-signer": "^1.1",
        "torann/geoip": "^1.0",
        "socialiteproviders/stackexchange": "^2.0",
        "lubusin/laravel-decomposer": "^1.1",
        "overtrue/wechat": "^3.2",
        "dingo/api": "1.0.x@dev",
        "appstract/laravel-opcache": "^1.1"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "barryvdh/laravel-ide-helper": "^2.1",
        "barryvdh/laravel-debugbar": "^2.2",
        "doctrine/dbal": "~2.3",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Support/helper.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
