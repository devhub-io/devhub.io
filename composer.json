{
    "name": "sysatom/devhub",
    "description": "devhub.io",
    "keywords": [
        "devhub"
    ],
    "license": "Private",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "laravel/framework": "5.7.*",
        "laravel/scout": "^5.0",
        "laravel/socialite": "^3.1",
        "laracasts/flash": "~2.0",
        "artesaos/seotools": "0.12.*",
        "prettus/l5-repository": "^2.6",
        "arcanedev/log-viewer": "~3.0",
        "spatie/laravel-analytics": "^2.1",
        "erusev/parsedown": "^1.6",
        "knplabs/github-api": "^1.7",
        "embed/embed": "^2.7",
        "guzzlehttp/guzzle": "^6.2",
        "league/fractal": "^0.13.0",
        "mcamara/laravel-localization": "^1.3",
        "rollbar/rollbar": "~0.18.0",
        "pragmarx/google2fa": "^0.7.1",
        "zizaco/entrust": "^1.7",
        "predis/predis": "^1.1",
        "intervention/image": "^2.3",
        "venturecraft/revisionable": "^1.28",
        "mailgun/mailgun-php": "^2.1",
        "mews/captcha": "^2.2",
        "laravel-notification-channels/pushover": "^0.0.2",
        "hieu-le/active": "^3.3",
        "torann/geoip": "^1.0",
        "socialiteproviders/stackexchange": "^3.0",
        "lubusin/laravel-decomposer": "^1.1",
        "overtrue/wechat": "^4.1",
        "appstract/laravel-opcache": "^1.1",
        "laravelium/sitemap": "3.0.*",
        "laravelium/feed": "3.0.*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.5",
        "beyondcode/laravel-dump-server": "^1.0",
        "doctrine/dbal": "~2.3",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "~1.4",
        "laravel/telescope": "^0.1.5",
        "mockery/mockery": "0.9.*",
        "nunomaduro/collision": "^2.0",
        "nunomaduro/larastan": "^0.3.7",
        "phpunit/phpunit": "^7.0",
        "symfony/css-selector": "4.*",
        "symfony/dom-crawler": "4.*"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Support/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
